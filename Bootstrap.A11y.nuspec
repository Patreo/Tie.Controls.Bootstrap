    <metadata>
        <id>Bootstrap.A11y</id>
        <version>1.1.0</version>
        <authors>kinzdesign,pmcfernandes</authors>
        <owners>kinzdesign,pmcfernandes</owners>
        <licenseUrl>https://raw.githubusercontent.com/wsomweb/Bootstrap.A11y/master/LICENSE</licenseUrl>
